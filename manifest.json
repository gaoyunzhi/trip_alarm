{"name": "Trip alarm",
 "description": "alarm you when road is clear",
 "version": "0.1",
 "background": {"scripts": ["moment.min.js", "bg.js"]},
 "options_page": "options.html",
 "browser_action": {
    "default_icon": "icon_white.png",
    "default_popup": "options.html"
  },
 "permissions": ["http://*.googleapis.com/", "https://*.googleapis.com/"],
 "content_security_policy": "default-src 'self' https://maps.googleapis.com",
 "manifest_version": 2}
