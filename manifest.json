{"name": "Trip alarm",
 "description": "alarm you when road is clear",
 "version": "0.1",
 "background": {"scripts": ["moment.min.js", "bg.js"], "persistent": false},
 "options_page": "options.html",
 "browser_action": {
    "default_icon": "icon_white.png",
    "default_popup": "options.html"
  },
 "permissions": ["http://*.googleapis.com/"],
 "content_security_policy": "script-src 'self' chrome-extension-resource: 'unsafe-eval'; default-src 'self'",
 "manifest_version": 3}
